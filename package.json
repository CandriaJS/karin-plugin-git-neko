    "markdown-it": "^14.1.0",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.13.14",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^9.23.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^15.15.0",
    "neostandard": "^0.12.1",
    "node-karin": "^1.7.14",
    "tsc-alias": "^1.8.11",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.28.0"
  },
  "files": [
    "config/",
    "lib/",
    "resources/",
    "LICENSE",
    "package.json",
    "README.md",
    "CHANGELOG.md"
